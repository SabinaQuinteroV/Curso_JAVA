
public class Ejerccio4 {

	public static void main(String[] args) {
		
		outer: for(int j=0; j<=20; j++) {
			System.out.println("Imprimiendo table del " + j + "\n");
			inner: for(int i=0; i<=10; i++) {
				if(i%2==0) {
					break inner;
				}
				System.out.println(j + " x " + i + " = " + (j*i));
			}
		}
	}

}
